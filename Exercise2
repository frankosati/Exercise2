#include <stdio.h>

    int main() {
    int num1;
    int num2;
    int choice;
    int p=0;
    
    while(p==0) {
        printf("Type 1 To Compare Two Numbers \n");
        printf("Type 2 For EXIT\n");
        scanf("%d" , &choice);
        
        if(choice==1){
            printf("Enter First Values: ");
            scanf("%d" , &num1);
            printf("Enter Second Values: ");
            scanf("%d" , &num2);
            
                if(num1>num2) {
                    printf("%d is Maximum : \n", num1,num2);
                }
                else if(num1<num2){
                    printf("%d is Minimum : \n" , num1,num2);
                }
                else if (num1=num2){
                    printf("%d is Equal : \n" , num1,num2);
                }
        }
        if (choice==2){
            p-1;
            printf("You Exit\n");
        }
        
    }


    return 0;
}
